   "source": [
    "import datetime\n",
    "\n",
    "def count_sundays():\n",
    "    sundays = 0\n",
    "    for year in range(1901, 2001):\n",
    "        for month in range(1, 13):\n",
    "            if datetime.date(year, month, 1).weekday() == 6:  # Sunday is 6\n",
    "                sundays += 1\n",
    "    return sundays\n",
    "\n",
    "print(count_sundays())\n"
   ]
